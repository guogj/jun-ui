#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/champion/data/ui/jun-ui/node_modules/.pnpm/release-it@17.0.0_typescript@5.3.2/node_modules/release-it/bin/node_modules:/Users/champion/data/ui/jun-ui/node_modules/.pnpm/release-it@17.0.0_typescript@5.3.2/node_modules/release-it/node_modules:/Users/champion/data/ui/jun-ui/node_modules/.pnpm/release-it@17.0.0_typescript@5.3.2/node_modules:/Users/champion/data/ui/jun-ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/champion/data/ui/jun-ui/node_modules/.pnpm/release-it@17.0.0_typescript@5.3.2/node_modules/release-it/bin/node_modules:/Users/champion/data/ui/jun-ui/node_modules/.pnpm/release-it@17.0.0_typescript@5.3.2/node_modules/release-it/node_modules:/Users/champion/data/ui/jun-ui/node_modules/.pnpm/release-it@17.0.0_typescript@5.3.2/node_modules:/Users/champion/data/ui/jun-ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../release-it/bin/release-it.js" "$@"
else
  exec node  "$basedir/../release-it/bin/release-it.js" "$@"
fi
